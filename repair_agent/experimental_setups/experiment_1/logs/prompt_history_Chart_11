Line 159:        if (l1 == null) {
Line 160:            return (l2 == null);
Line 161:        }
Line 162:        if (l2 == null) {
Line 163:            return false;
Line 164:        }
Line 165:        if (!l1.getP1().equals(l2.getP1())) {
Line 166:            return false;
Line 167:        }
Line 168:        if (!l1.getP2().equals(l2.getP2())) {
Line 169:            return false;
Line 170:        }
Line 171:        return true;
Line 172:    }
