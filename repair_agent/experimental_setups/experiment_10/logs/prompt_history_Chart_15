    protected Paint lookupSectionPaint(Comparable key, boolean autoPopulate) {
        Paint result = null;
        
        // is a paint defined for the specified key
        result = this.sectionPaintMap.getPaint(key);
        if (result != null) {
            return result;
        }
        
