    {
      try {
          Field[] fields = Color.class.getFields();
          for (int i = 0; i < fields.length; i++) {
              Field f = fields[i];
              if (Modifier.isPublic(f.getModifiers())
                  && Modifier.isFinal(f.getModifiers())
                  && Modifier.isStatic(f.getModifiers())) {
