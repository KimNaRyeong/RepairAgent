      StringBuffer retval = new StringBuffer(7);
      retval.append("#");

      int fillUp = 6 - color.length();
      for (int i = 0; i < fillUp; i++) {
          retval.append("0");
      }

      retval.append(color);
      return retval.toString();
    }

