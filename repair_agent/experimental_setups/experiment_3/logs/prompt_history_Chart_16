                "new_lines": [
                    "    // ... additional new lines ...\n"
                ]
            }
        ],
        "deletions": [179, 183],
        "modifications": [
            {
                "line_number": 179,
                "modified_line": "    if (dataset == null) {\n"
            },
            {
